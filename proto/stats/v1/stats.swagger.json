{
  "swagger": "2.0",
  "info": {
    "title": "Stats APIs",
    "description": "Stats APIs",
    "version": "1.0",
    "contact": {
      "name": "Arktos Venture project",
      "url": "https://github.com/arktos-venture/buf",
      "email": "oss@arktos-consulting.cloud"
    }
  },
  "tags": [
    {
      "name": "Stats"
    }
  ],
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/stats": {
      "delete": {
        "summary": "Private API\nDelete Stat",
        "operationId": "Stats_Delete",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1StatDelete"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "tsdb",
            "in": "query",
            "required": false,
            "type": "string",
            "enum": [
              "Companies",
              "Forexes",
              "Indices",
              "Indexes",
              "Accounts"
            ],
            "default": "Companies"
          },
          {
            "name": "exchanges",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "tickers",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          }
        ],
        "tags": [
          "Stats"
        ]
      }
    },
    "/v1/stats/{exchange}/{ticker}": {
      "get": {
        "summary": "Public API\nGet Stat properties",
        "operationId": "Stats_Last",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1StatReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "exchange",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "ticker",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "tsdb",
            "in": "query",
            "required": false,
            "type": "string",
            "enum": [
              "Companies",
              "Forexes",
              "Indices",
              "Indexes",
              "Accounts"
            ],
            "default": "Companies"
          }
        ],
        "tags": [
          "Stats"
        ]
      },
      "put": {
        "summary": "Private API\nUpdate properties of Stat",
        "operationId": "Stats_Update",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1StatReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "exchange",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "ticker",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "tsdb": {
                  "$ref": "#/definitions/v1TSDB"
                },
                "data": {
                  "$ref": "#/definitions/StatUpdateRequestData"
                }
              }
            }
          }
        ],
        "tags": [
          "Stats"
        ]
      }
    }
  },
  "definitions": {
    "StatUpdateRequestData": {
      "type": "object",
      "properties": {
        "price": {
          "$ref": "#/definitions/statsv1Price"
        },
        "volume": {
          "$ref": "#/definitions/v1Volume"
        }
      }
    },
    "googlerpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "type_url": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "statsv1Price": {
      "type": "object",
      "properties": {
        "max_annual": {
          "type": "number",
          "format": "float"
        },
        "min_annual": {
          "type": "number",
          "format": "float"
        },
        "return_year": {
          "type": "number",
          "format": "float"
        },
        "market_cap_usd": {
          "type": "string",
          "format": "int64"
        },
        "beta5y": {
          "type": "number",
          "format": "float"
        },
        "start": {
          "type": "number",
          "format": "float"
        }
      }
    },
    "v1StatDelete": {
      "type": "object",
      "properties": {
        "total": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "v1StatReply": {
      "type": "object",
      "properties": {
        "ticker": {
          "type": "string"
        },
        "exchange": {
          "type": "string"
        },
        "price": {
          "$ref": "#/definitions/statsv1Price"
        },
        "volume": {
          "$ref": "#/definitions/v1Volume"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "updated_at": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "v1TSDB": {
      "type": "string",
      "enum": [
        "Companies",
        "Forexes",
        "Indices",
        "Indexes",
        "Accounts"
      ],
      "default": "Companies"
    },
    "v1Volume": {
      "type": "object",
      "properties": {
        "volumeAvg10d": {
          "type": "string",
          "format": "int64"
        },
        "volumeAvg30d": {
          "type": "string",
          "format": "int64"
        },
        "volumeAvg90d": {
          "type": "string",
          "format": "int64"
        }
      }
    }
  },
  "securityDefinitions": {
    "bearer": {
      "type": "apiKey",
      "description": "Authentication token, prefixed by Bearer: Bearer \u003ctoken\u003e",
      "name": "Authorization",
      "in": "header"
    }
  },
  "security": [
    {
      "bearer": []
    }
  ]
}
