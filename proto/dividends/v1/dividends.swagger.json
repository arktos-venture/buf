        },
        "record_date": {
          "type": "string",
          "format": "date-time"
        },
        "payment_date": {
          "type": "string",
          "format": "date-time"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "v1DividendReply": {
      "type": "object",
      "properties": {
        "values": {
          "type": "array",
          "items": {
            "type": "number",
            "format": "float"
          }
        },
        "yields": {
          "type": "array",
          "items": {
            "type": "number",
            "format": "float"
          }
        },
        "declaration_date": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "date-time"
          }
        },
        "record_date": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "date-time"
          }
        },
        "payment_date": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "date-time"
          }
        },
        "created_at": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    },
    "v1DividendRequest": {
      "type": "object",
      "properties": {
        "interval": {
          "$ref": "#/definitions/v1Interval"
        },
        "filters": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1Filter"
          }
        }
      }
    },
    "v1Filter": {
      "type": "object",
      "properties": {
        "operator": {
          "$ref": "#/definitions/v1Operator"
        },
        "argument": {
          "$ref": "#/definitions/v1Argument"
        },
        "values": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "v1Interval": {
      "type": "string",
      "enum": [
        "d",
        "w",
        "m",
        "q",
        "y"
      ],
      "default": "d"
    },
    "v1Operator": {
      "type": "string",
      "enum": [
        "Equal",
        "NotEqual",
        "Greater",
        "GreaterEqual",
        "Less",
        "LessEqual",
        "In",
        "NotIn",
        "Between"
      ],
      "default": "Equal"
    }
  },
  "securityDefinitions": {
    "bearer": {
      "type": "apiKey",
      "description": "Authentication token, prefixed by Bearer: Bearer \u003ctoken\u003e",
      "name": "Authorization",
      "in": "header"
    }
