  "tags": [
    {
      "name": "Dividends"
    }
  ],
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/dividend/{exchange}/{ticker}": {
      "get": {
        "summary": "Public API\nGet Last dividend result",
        "operationId": "Dividends_Last",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1DividendLastReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "exchange",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "ticker",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "Dividends"
        ]
      }
    },
    "/v1/dividends": {
      "delete": {
        "summary": "Private API\nDelete dividends by exchanges or tickers",
        "operationId": "Dividends_Delete",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1DividendDelete"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "tickers",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "exchanges",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          }
        ],
        "tags": [
          "Dividends"
        ]
      },
      "post": {
        "summary": "Public API\nSearch dividends by date",
        "operationId": "Dividends_Search",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1DividendReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1DividendRequest"
            }
          }
        ],
        "tags": [
          "Dividends"
        ]
      }
    }
  },
  "definitions": {
    "googlerpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "type_url": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "v1Argument": {
      "type": "string",
      "enum": [
        "Ticker",
        "Exchange",
        "Industry",
        "Index",
        "Currency",
        "MarketCap",
        "ReturnYear",
        "Beta5Y",
        "ShareVolumeRatio",
        "DividendYield",
        "Price",
        "PriceLastChange",
        "Strategy",
        "StrategyPeriod",
        "StrategyDecision",
        "StrategyDecisionAgo",
        "SharesFloatRatio",
        "Volume",
        "VolumeAvg10d",
        "VolumeAvg30d",
        "VolumeAvg90d",
        "Isin",
        "Adr",
        "Id",
        "Country",
        "Time",
        "Action",
        "Duration",
        "Type",
        "Status"
      ],
      "default": "Ticker"
    },
    "v1DividendDelete": {
      "type": "object",
      "properties": {
        "total": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "v1DividendLastReply": {
      "type": "object",
      "properties": {
        "values": {
