            }
          }
        ],
        "tags": [
          "Quotes"
        ]
      }
    },
    "/v1/quotes/{exchange}/{ticker}": {
      "get": {
        "operationId": "Quotes_Last",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1QuotesLastReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "exchange",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "ticker",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "tsdb",
            "in": "query",
            "required": false,
            "type": "string",
            "enum": [
              "Companies",
              "Forexes",
              "Indices",
              "Indexes",
              "Accounts"
            ],
            "default": "Companies"
          }
        ],
        "tags": [
          "Quotes"
        ]
      }
    }
