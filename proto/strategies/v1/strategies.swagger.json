            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1StrategyUpdateRequest"
            }
          }
        ],
        "tags": [
          "Strategies"
        ]
      }
    },
    "/v1/strategy/{ticker}": {
      "get": {
        "summary": "Public API\nGet Strategy properties",
        "operationId": "Strategies_Get",
        "responses": {
