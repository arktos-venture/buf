  "paths": {
    "/v1/strategies": {
      "get": {
        "summary": "Public API\nList Strategies",
        "operationId": "Strategies_List",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1StrategyReplies"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "account",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "Strategies"
        ]
      },
      "delete": {
        "summary": "Public API\nDelete Strategy",
        "operationId": "Strategies_Delete",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1StrategyDelete"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "account",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "tickers",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          }
        ],
        "tags": [
          "Strategies"
        ]
      },
      "post": {
        "summary": "Public API\nCreate Strategy",
        "operationId": "Strategies_Create",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1StrategyReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1StrategyModifyRequest"
            }
          }
        ],
        "tags": [
          "Strategies"
        ]
      }
    },
    "/v1/strategies/search": {
      "post": {
        "summary": "Public API\nSearch Strategies",
        "operationId": "Strategies_Search",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1StrategySearchReplies"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1StrategySearchRequest"
            }
          }
        ],
        "tags": [
          "Strategies"
        ]
      }
    },
    "/v1/strategy/{ticker}": {
      "get": {
        "summary": "Public API\nGet Strategy properties",
        "operationId": "Strategies_Get",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1StrategyReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "ticker",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "account",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "Strategies"
        ]
      },
      "put": {
        "summary": "Public API\nUpdate Strategy properties",
        "operationId": "Strategies_Update",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1StrategyReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "ticker",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "account": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "parameters": {
                  "$ref": "#/definitions/v1Parameters"
                }
              }
            }
          }
        ],
        "tags": [
          "Strategies"
        ]
      }
    }
  },
  "definitions": {
    "googlerpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "type_url": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "v1Argument": {
      "type": "string",
      "enum": [
        "Ticker",
        "Exchange",
        "Industry",
        "Index",
        "Currency",
        "MarketCap",
        "ReturnYear",
        "Beta5Y",
        "ShareVolumeRatio",
        "DividendYield",
        "Price",
        "PriceLastChange",
        "Strategy",
        "StrategyPeriod",
        "StrategyDecision",
        "StrategyDecisionAgo",
        "SharesFloatRatio",
        "Volume",
        "VolumeAvg10d",
        "VolumeAvg30d",
        "VolumeAvg90d",
        "Isin",
        "Adr",
        "Id",
        "Country",
        "Time",
        "Action",
        "Duration",
        "Type",
        "Status"
      ],
      "default": "Ticker"
    },
    "v1FilterSimple": {
      "type": "object",
      "properties": {
        "operator": {
          "$ref": "#/definitions/v1Operator"
        },
        "argument": {
          "$ref": "#/definitions/v1Argument"
        },
        "value": {
          "type": "string"
        }
      }
    },
    "v1Interval": {
      "type": "string",
      "enum": [
        "d",
        "w",
        "m",
        "q",
        "y"
      ],
      "default": "d"
    },
    "v1Operator": {
      "type": "string",
      "enum": [
        "Equal",
        "NotEqual",
        "Greater",
        "GreaterEqual",
        "Less",
        "LessEqual",
        "In",
        "NotIn",
        "Between"
      ],
      "default": "Equal"
    },
    "v1OrderAction": {
      "type": "string",
      "enum": [
        "BUY",
        "SELL",
        "HOLD"
      ],
      "default": "BUY"
    },
    "v1Parameters": {
      "type": "object",
      "properties": {
        "values": {
          "type": "object",
          "additionalProperties": {
            "type": "number",
            "format": "float"
          }
        },
        "period": {
          "type": "object",
          "additionalProperties": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "title": "Transverse"
    },
    "v1StrategyDelete": {
      "type": "object",
      "properties": {
        "total": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "v1StrategyModifyRequest": {
      "type": "object",
      "properties": {
        "account": {
          "type": "string"
        },
        "ticker": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "parameters": {
          "$ref": "#/definitions/v1Parameters"
        }
      }
    },
    "v1StrategyReplies": {
      "type": "object",
      "properties": {
        "results": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1StrategyRepliesResult"
          }
        },
        "total": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "v1StrategyRepliesResult": {
      "type": "object",
      "properties": {
        "ticker": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "updated_at": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "v1StrategyReply": {
      "type": "object",
      "properties": {
        "ticker": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "parameters": {
          "$ref": "#/definitions/v1Parameters"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "updated_at": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "v1StrategySearchReplies": {
      "type": "object",
      "properties": {
        "Results": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/v1StrategySearchRepliesResult"
          }
        },
        "total": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "v1StrategySearchRepliesResult": {
      "type": "object",
      "properties": {
        "period": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1Interval"
          }
        },
        "status": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1OrderAction"
          }
        },
        "breakout": {
          "type": "array",
          "items": {
            "type": "number",
            "format": "float"
          }
        },
        "status_ago": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "date-time"
          }
        },
        "created_at": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "date-time"
          }
        },
        "updated_at": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "date-time"
          }
        }
      }
    },
    "v1StrategySearchRequest": {
      "type": "object",
      "properties": {
        "account": {
          "type": "string"
        },
        "tsdb": {
          "$ref": "#/definitions/v1TSDB"
        },
        "filters": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1FilterSimple"
          }
        }
      }
    },
    "v1TSDB": {
      "type": "string",
      "enum": [
        "Companies",
        "Forexes",
        "Indices",
        "Indexes",
        "Accounts"
      ],
      "default": "Companies"
    }
  },
  "securityDefinitions": {
    "bearer": {
      "type": "apiKey",
      "description": "Authentication token, prefixed by Bearer: Bearer \u003ctoken\u003e",
      "name": "Authorization",
      "in": "header"
    }
  },
  "security": [
    {
      "bearer": []
    }
  ]
}
