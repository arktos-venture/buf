{
  "swagger": "2.0",
  "info": {
    "title": "Strategies APIs",
    "description": "Strategies APIs",
    "version": "1.0",
    "contact": {
      "name": "Arktos Venture project",
      "url": "https://github.com/arktos-venture/buf",
      "email": "oss@arktos-consulting.cloud"
    }
  },
  "tags": [
    {
      "name": "Strategies"
    }
  ],
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/strategies": {
      "get": {
        "operationId": "Strategies_List",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1StrategyReplies"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "tags": [
          "Strategies"
        ]
      }
    },
    "/v1/strategy": {
      "post": {
        "operationId": "Strategies_Create",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/strategiesv1StrategyReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1StrategyUpdateRequest"
            }
          }
        ],
        "tags": [
          "Strategies"
        ]
      }
    },
    "/v1/strategy/{strategy.ticker}/execute": {
      "post": {
        "operationId": "Strategies_Execute",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1StrategyExecutedReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "strategy.ticker",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1StrategyExecuteRequest"
            }
          }
        ],
        "tags": [
          "Strategies"
        ]
      }
    },
    "/v1/strategy/{ticker}": {
      "get": {
        "operationId": "Strategies_Get",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/strategiesv1StrategyReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "ticker",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "Strategies"
        ]
      },
      "delete": {
        "operationId": "Strategies_Delete",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "properties": {}
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "ticker",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "Strategies"
        ]
      },
      "put": {
        "operationId": "Strategies_Update",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/strategiesv1StrategyReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "ticker",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "account": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "parameters": {
                  "$ref": "#/definitions/strategiesv1Request"
                }
              }
            }
          }
        ],
        "tags": [
          "Strategies"
        ]
      }
    }
  },
  "definitions": {
    "StrategyExecuteRequestStrategy": {
      "type": "object",
      "properties": {
        "ticker": {
          "type": "string"
        },
        "params": {
          "type": "object",
          "additionalProperties": {
            "type": "integer",
            "format": "int32"
          }
        },
        "inject": {
          "type": "number",
          "format": "float"
        }
      }
    },
    "googlerpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "type_url": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "quotesv1Interval": {
      "type": "string",
      "enum": [
        "d",
        "w",
        "m",
        "y"
      ],
      "default": "d"
    },
    "screenerv1Argument": {
      "type": "string",
      "enum": [
        "Ticker",
        "Exchange",
        "Industry",
        "Index",
        "Currency",
        "MarketCap",
        "ReturnYear",
        "Beta5Y",
        "ShareVolumeRatio",
        "DividendYield",
        "Price",
        "PriceLastChange",
        "Strategy",
        "StrategyPeriod",
        "StrategyDecision",
        "StrategyDecisionAgo",
        "SharesFloatRatio",
        "Volume",
        "VolumeAvg10d",
        "VolumeAvg30d",
        "VolumeAvg90d",
        "Isin",
        "Adr",
        "Id",
        "Country",
        "Time"
      ],
      "default": "Ticker"
    },
    "screenerv1Operator": {
      "type": "string",
      "enum": [
        "Equal",
        "NotEqual",
        "Greater",
        "GreaterEqual",
        "Less",
        "LessEqual",
        "In",
        "NotIn",
        "Between"
      ],
      "default": "Equal"
    },
    "strategiesv1Request": {
      "type": "object",
      "properties": {
        "parameters": {
          "type": "object",
          "additionalProperties": {
            "type": "integer",
            "format": "int32"
          }
        },
        "period": {
          "type": "object",
          "additionalProperties": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "title": "Transverse"
    },
    "strategiesv1StrategyReply": {
      "type": "object",
      "properties": {
        "ticker": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "account": {
          "type": "string"
        },
        "request": {
          "$ref": "#/definitions/strategiesv1Request"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "updated_at": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "v1FilterSimple": {
      "type": "object",
      "properties": {
        "operator": {
          "$ref": "#/definitions/screenerv1Operator"
        },
        "argument": {
          "$ref": "#/definitions/screenerv1Argument"
        },
        "value": {
          "type": "string"
        }
      }
    },
    "v1StrategyExecuteRequest": {
      "type": "object",
      "properties": {
        "tsdb": {
          "$ref": "#/definitions/v1TSDB"
        },
        "interval": {
          "$ref": "#/definitions/quotesv1Interval"
        },
        "strategy": {
          "$ref": "#/definitions/StrategyExecuteRequestStrategy"
        },
        "filters": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1FilterSimple"
          }
        }
      }
    },
    "v1StrategyExecutedReply": {
      "type": "object",
      "properties": {
        "action": {
          "$ref": "#/definitions/v1StrategyExecutedReplyAction"
        },
        "date": {
          "type": "string",
          "format": "date-time"
        },
        "last_quote": {
          "type": "number",
          "format": "float"
        },
        "last_quote_date": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "v1StrategyExecutedReplyAction": {
      "type": "string",
      "enum": [
        "BUY",
        "SELL",
        "HOLD"
      ],
      "default": "BUY"
    },
    "v1StrategyReplies": {
      "type": "object",
      "properties": {
        "results": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/strategiesv1StrategyReply"
          }
        },
        "total": {
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "v1StrategyUpdateRequest": {
      "type": "object",
      "properties": {
        "ticker": {
          "type": "string"
        },
        "account": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "parameters": {
          "$ref": "#/definitions/strategiesv1Request"
        }
      }
    },
    "v1TSDB": {
      "type": "string",
      "enum": [
        "Companies",
        "Forexes",
        "Indices",
        "Indexes",
        "Accounts"
      ],
      "default": "Companies"
    }
  },
  "securityDefinitions": {
    "bearer": {
      "type": "apiKey",
      "description": "Authentication token, prefixed by Bearer: Bearer \u003ctoken\u003e",
      "name": "Authorization",
      "in": "header"
    }
  },
  "security": [
    {
      "bearer": []
    }
  ]
}
