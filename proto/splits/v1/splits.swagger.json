        "operationId": "Splits_Last",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1SplitsLastReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
