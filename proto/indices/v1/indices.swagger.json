          "format": "float"
        },
        "low": {
          "type": "number",
          "format": "float"
        },
        "high": {
          "type": "number",
          "format": "float"
        },
        "volume": {
          "type": "number",
          "format": "float"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        }
      }
