    "v1ScreenerReply": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "int64"
        },
        "ticker": {
          "type": "string"
        },
        "ticker_alternative": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "isin": {
          "type": "string"
        },
        "exchange": {
          "type": "string"
        },
        "adr": {
          "type": "boolean"
