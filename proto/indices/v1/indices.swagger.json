        "economicSector": {
          "type": "string",
          "format": "int64"
        },
        "businessSector": {
          "type": "string",
          "format": "int64"
        },
        "industryGroup": {
          "type": "string",
          "format": "int64"
        },
        "industry": {
          "type": "string",
          "format": "int64"
        },
        "activity": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "ScreenerReplyDividends": {
