        "activity": {
          "$ref": "#/definitions/ScreenerReplyActivity"
        },
        "shares": {
          "$ref": "#/definitions/ScreenerReplyShares"
        },
        "pricing": {
          "$ref": "#/definitions/ScreenerReplyPricing"
        },
        "dividends": {
          "$ref": "#/definitions/ScreenerReplyDividends"
        },
        "volume": {
          "$ref": "#/definitions/v1ScreenerReplyVolume"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "updated_at": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "v1ScreenerReplyVolume": {
      "type": "object",
      "properties": {
        "last": {
