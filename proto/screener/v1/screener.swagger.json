{
  "swagger": "2.0",
  "info": {
    "title": "Screener APIs",
    "description": "Screener APIs",
    "version": "1.0",
    "contact": {
      "name": "Arktos Venture project",
      "url": "https://github.com/arktos-venture/buf",
      "email": "oss@arktos-consulting.cloud"
    }
  },
  "tags": [
    {
      "name": "Screener"
    }
  ],
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/screener": {
      "post": {
        "summary": "Public API",
        "operationId": "Screener_Search",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1ScreenerReplies"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1ScreenerRequest"
            }
          }
        ],
        "tags": [
          "Screener"
        ]
      }
    }
  },
  "definitions": {
    "ScreenerReplyActivity": {
      "type": "object",
      "properties": {
        "economicSector": {
          "type": "string",
          "format": "int64"
        },
        "businessSector": {
          "type": "string",
          "format": "int64"
        },
        "industryGroup": {
          "type": "string",
          "format": "int64"
        },
        "industry": {
          "type": "string",
          "format": "int64"
        },
        "activity": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "ScreenerReplyDividends": {
      "type": "object",
      "properties": {
        "yield": {
          "type": "number",
          "format": "float"
        },
        "annual": {
          "type": "number",
          "format": "float"
        }
      }
    },
    "ScreenerReplyPricing": {
      "type": "object",
      "properties": {
        "last": {
          "type": "number",
          "format": "float"
        },
        "minYear": {
          "type": "number",
          "format": "float"
        },
        "maxYear": {
          "type": "number",
          "format": "float"
        },
        "returnYear": {
          "type": "number",
          "format": "float"
        },
        "marketcap": {
          "type": "string",
          "format": "int64"
        },
        "beta5y": {
          "type": "number",
          "format": "float"
        }
      }
    },
    "ScreenerReplyShares": {
      "type": "object",
      "properties": {
        "outstanding": {
          "type": "string",
          "format": "int64"
        },
        "float": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "ScreenerReplyVolume": {
      "type": "object",
      "properties": {
        "last": {
          "type": "number",
          "format": "float"
        },
        "stats": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/VolumeStats"
          }
        }
      }
    },
    "SortOrientation": {
      "type": "string",
      "enum": [
        "ASC",
        "DESC"
      ],
      "default": "ASC"
    },
    "VolumeStats": {
      "type": "object",
      "properties": {
        "avg": {
          "type": "number",
          "format": "float"
        },
        "variation": {
          "type": "number",
          "format": "float"
        }
      }
    },
    "googlerpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "type_url": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "screenerv1Argument": {
      "type": "string",
      "enum": [
        "Ticker",
        "Exchange",
        "Industry",
        "Index",
        "Currency",
        "MarketCap",
        "ReturnYear",
        "Beta5Y",
        "ShareVolumeRatio",
        "DividendYield",
        "Price",
        "PriceLastChange",
        "Strategy",
        "StrategyPeriod",
        "StrategyDecision",
        "StrategyDecisionAgo",
        "SharesFloatRatio",
        "Volume",
        "VolumeAvg10d",
        "VolumeAvg30d",
        "VolumeAvg90d",
        "Isin",
        "Adr",
        "Id",
        "Country",
        "Time"
      ],
      "default": "Ticker"
    },
    "screenerv1Filter": {
      "type": "object",
      "properties": {
        "operator": {
          "$ref": "#/definitions/screenerv1Operator"
        },
        "argument": {
          "$ref": "#/definitions/screenerv1Argument"
        },
        "values": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "screenerv1Operator": {
      "type": "string",
      "enum": [
        "Equal",
        "NotEqual",
        "Greater",
        "GreaterEqual",
        "Less",
        "LessEqual",
        "In",
        "NotIn",
        "Between"
      ],
      "default": "Equal"
    },
    "v1ScreenerReplies": {
      "type": "object",
      "properties": {
        "results": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1ScreenerReply"
          }
        },
        "total": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "v1ScreenerReply": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "int64"
        },
        "ticker": {
          "type": "string"
        },
        "ticker_alternative": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "isin": {
          "type": "string"
        },
        "exchange": {
          "type": "string"
        },
        "adr": {
          "type": "boolean"
        },
        "activity": {
          "$ref": "#/definitions/ScreenerReplyActivity"
        },
        "shares": {
          "$ref": "#/definitions/ScreenerReplyShares"
        },
        "pricing": {
          "$ref": "#/definitions/ScreenerReplyPricing"
        },
        "dividends": {
          "$ref": "#/definitions/ScreenerReplyDividends"
        },
        "volume": {
          "$ref": "#/definitions/ScreenerReplyVolume"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "updated_at": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "v1ScreenerRequest": {
      "type": "object",
      "properties": {
        "filters": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/screenerv1Filter"
          }
        },
        "sort": {
          "$ref": "#/definitions/v1ScreenerRequestSort"
        },
        "limit": {
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "v1ScreenerRequestSort": {
      "type": "object",
      "properties": {
        "argument": {
          "$ref": "#/definitions/screenerv1Argument"
        },
        "orientation": {
          "$ref": "#/definitions/SortOrientation"
        }
      }
    }
  },
  "securityDefinitions": {
    "bearer": {
      "type": "apiKey",
      "description": "Authentication token, prefixed by Bearer: Bearer \u003ctoken\u003e",
      "name": "Authorization",
      "in": "header"
    }
  },
  "security": [
    {
      "bearer": []
    }
  ]
}
