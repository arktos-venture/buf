  "paths": {
    "/v1/company/{exchange}/{ticker}": {
      "get": {
        "summary": "Public API",
        "operationId": "Companies_Get",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1CompanyReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "exchange",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "name": "ticker",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "Companies"
        ]
      }
    },
    "/v1/company/{exchange}/{ticker}/similars": {
      "get": {
        "summary": "Public API",
        "operationId": "Companies_Similars",
        "responses": {
          "200": {
            "description": "A successful response.",
