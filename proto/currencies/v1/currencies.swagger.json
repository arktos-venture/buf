{
  "swagger": "2.0",
  "info": {
    "title": "Currencies APIs",
    "description": "Currencies APIs",
    "version": "1.0",
    "contact": {
      "name": "Arktos Venture project",
      "url": "https://github.com/arktos-venture/buf",
      "email": "oss@arktos-consulting.cloud"
    }
  },
  "tags": [
    {
      "name": "Currencies"
    }
  ],
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/currencies": {
      "get": {
        "summary": "Public API: List Currencies available",
        "operationId": "Currencies_List",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1CurrencyReplies"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "tags": [
          "Currencies"
        ]
      }
    },
    "/v1/currency/{ticker}": {
      "get": {
        "summary": "Public API: Get Currency properties",
        "operationId": "Currencies_Get",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1CurrencyReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "ticker",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "Currencies"
        ]
      }
    }
  },
  "definitions": {
    "CentralBankBorrowingRate": {
      "type": "object",
      "properties": {
        "rate": {
          "type": "number",
          "format": "float"
        },
        "since": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "CurrencyReplyCentralBank": {
      "type": "object",
      "properties": {
        "ticker": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "borrowing_rate": {
          "$ref": "#/definitions/CentralBankBorrowingRate"
        }
      }
    },
    "googlerpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "type_url": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "v1CountryRepliesResult": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "region": {
          "type": "string"
        },
        "continent": {
          "type": "string"
        },
        "alpha2": {
          "type": "string"
        }
      }
    },
    "v1CurrencyReplies": {
      "type": "object",
      "properties": {
        "results": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "total": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "v1CurrencyReply": {
      "type": "object",
      "properties": {
        "ticker": {
          "type": "string"
        },
        "central_bank": {
          "$ref": "#/definitions/CurrencyReplyCentralBank"
        },
        "forexes": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "exchanges": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1ExchangeRepliesResult"
          }
        },
        "countries": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1CountryRepliesResult"
          }
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "updated_at": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "v1ExchangeRepliesResult": {
      "type": "object",
      "properties": {
        "ticker": {
          "type": "string"
        },
        "routing": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "open": {
          "type": "boolean"
        }
      }
    }
  },
  "securityDefinitions": {
    "bearer": {
      "type": "apiKey",
      "description": "Authentication token, prefixed by Bearer: Bearer \u003ctoken\u003e",
      "name": "Authorization",
      "in": "header"
    }
  },
  "security": [
    {
      "bearer": []
    }
  ]
}
