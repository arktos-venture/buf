  "paths": {
    "/v1/currencies": {
      "get": {
        "summary": "Public API\nList Currencies available",
        "operationId": "Currencies_List",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1CurrencyReplies"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "tags": [
          "Currencies"
        ]
      },
      "delete": {
        "summary": "Private API\nDelete Currency \u0026 deps",
        "operationId": "Currencies_Delete",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1CurrencyDelete"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "tickers",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          }
        ],
        "tags": [
          "Currencies"
        ]
      }
    },
    "/v1/currency": {
      "post": {
        "summary": "Private API\nCreate Currency",
        "operationId": "Currencies_Create",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1CurrencyReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/v1CurrencyRequest"
            }
          }
        ],
        "tags": [
          "Currencies"
        ]
      }
    },
    "/v1/currency/{ticker}": {
      "get": {
        "summary": "Public API\nGet Currency properties",
        "operationId": "Currencies_Get",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1CurrencyReply"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "ticker",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "Currencies"
        ]
      }
    }
  },
  "definitions": {
    "googlerpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "type_url": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    },
    "v1CurrencyDelete": {
      "type": "object",
      "properties": {
        "total": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "v1CurrencyReplies": {
      "type": "object",
      "properties": {
        "results": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "total": {
          "type": "string",
          "format": "int64"
        }
      }
    },
    "v1CurrencyReply": {
      "type": "object",
      "properties": {
        "ticker": {
          "type": "string"
        },
        "exchanges": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "v1CurrencyRequest": {
      "type": "object",
      "properties": {
        "ticker": {
          "type": "string"
        }
      }
    }
  },
  "securityDefinitions": {
    "bearer": {
      "type": "apiKey",
      "description": "Authentication token, prefixed by Bearer: Bearer \u003ctoken\u003e",
